<Window x:Class="ShoeShop.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ShoeShop"
        mc:Ignorable="d"
        Title="ShoeShop" ResizeMode="NoResize" Height="450" Width="800" WindowStartupLocation="CenterScreen" Cursor="Arrow" FontFamily="Tw Cen MT Condensed Extra Bold">

    <StackPanel Background="#FF2B1414">
        <Button BorderThickness="1" BorderBrush="#ffe70d" Background="#000000" Foreground="White"  x:Name="powrot" Margin="600,25,0,0" Width="130" Height="25" Content="Login" Click="powrot_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="175,43,0,40" TextWrapping="Wrap" Text="Nadchodzące premiery" FontSize="50" Foreground="white" Height="67" Width="461" />
        <DataGrid Name="premiery" Height="185" d:ItemsSource="{Binding}" Margin="0,-20,0,0" FontSize="24" Width="670"/>
    </StackPanel>


</Window>
